<odoo>
    <data>
        <!-- explicit list view definition -->

        <record model="ir.ui.view" id="final_project.list">
            <field name="name">Taxes list</field>
            <field name="model">final.report</field>
            <field name="arch" type="xml">
                <tree decoration-info="target_move in ('posted','all')">
                    <field name="final_customer" widget="many2one_avatar"/>
                    <field name="date_from"/>
                    <field name="date_to"/>
                    <field name="final_accountant"/>
                    <field name="final_create_date"/>
                    <field name="target_move" widget='badge' decoration-success="target_move == 'posted'" decoration-danger="target_move == 'all'"/>


                </tree>
            </field>
        </record>

        <record id="final_tax_view" model="ir.ui.view">
            <field name="name">tax Report</field>
            <field name="model">final.report</field>
            <field name="arch" type="xml">
                <form string="Report Options">
                    <group col="4">
                        <field name="final_customer" widget="many2one_avatar"/>
                        <field name="target_move" widget="radio"/>
                        <field name="date_from"/>
                        <field name="date_to"/>

                    </group>
                    <group>
                        <field name="final_accountant"/>
                        <field name="final_create_date"/>
                        <field name="journal_ids" widget="many2many_tags"/>
                        <field name="company_id" options="{'no_create': True}" groups="base.group_multi_company"/>
                    </group>
                    <footer>
                        <button name="check_report" string="Print" type="object" default_focus="1"
                                class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>
