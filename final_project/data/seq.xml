<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <data noupdate="1">

<!--        Tax report sequence start by TA and has 6 number digits-->
        <record id="tax_report" model="ir.sequence">
            <field name="name">Tax account</field>
            <field name="code">custom.tax.sequence</field>
            <field name="prefix">TA</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
<!--        Tax report sequence start by PL and has 6 number digits-->
        <record id="profitloss_seq" model="ir.sequence">
            <field name="name">Profit and Loss account</field>
            <field name="code">custom.profitloss.sequence</field>
            <field name="prefix">PL</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
<!--        Tax report sequence start by AC and has 6 number digits-->
        <record id="accountant_seq" model="ir.sequence">
            <field name="name">Accountants Report</field>
            <field name="code">custom.accountant.sequence</field>
            <field name="prefix">AC</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
<!--        Tax report sequence start by SY and has 6 number digits-->
        <record id="system_seq" model="ir.sequence">
            <field name="name">System Report</field>
            <field name="code">custom.system.sequence</field>
            <field name="prefix">SY</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>